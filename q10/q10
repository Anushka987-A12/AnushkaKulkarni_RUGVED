def valid_cardno(num):
    n=num[::-1]
    n1=[int(x) for x in n]
    for i in range(1,len(n1),2):
        n1[i]=n1[i]*2
        if 9 < n1[i]:
            n1[i]=n1[i] % 10 + 1
    total=sum(n1)
    if total%10 == 0:
        print("valid card number")
    else:
        print("invalid card number")
i=input("Enter card number :")
valid_cardno(i)