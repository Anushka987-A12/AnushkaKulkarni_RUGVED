def anagram(s1,s2):
    str1=sorted(s1.lower().replace(" ",""))
    str2=sorted(s2.lower().replace(" ",""))
    if str1==str2:
        print("yes the given strings are anagrams of each other")
    else:
        print("sorry the strings are not anagrams")
a=input("Enter string 1: ")
b=input("Enter string 2: ")
anagram(a,b)
    